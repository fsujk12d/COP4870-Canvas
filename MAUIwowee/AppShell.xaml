<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MAUIwowee.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:MAUIwowee"
    Shell.FlyoutBehavior="Disabled"
    Title="MAUIwowee">

    <ShellContent
        Title="Home"
        ContentTemplate="{DataTemplate local:MainPage}"
        Route="MainPage" />
    <ShellContent
    Title="Instructor"
    ContentTemplate="{DataTemplate local2:InstructorView}"
    Route="Instructor" />

</Shell>
